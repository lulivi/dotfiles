# 1 "/home/luvo/.Xresources.theme" 1
# 8 "/home/luvo/.Xresources.src" 2






!#define background_opacity 95

*.font: Fira Code
*.font_size: 10
*.border_width: 10
*.window_gap: 10

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! Colors
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

*foreground: #c0c5ce



*background: #2b303b

*cursorColor: #c0c5ce

*color0: #2b303b
*color1: #bf616a
*color2: #a3be8c
*color3: #ebcb8b
*color4: #8fa1b3
*color5: #b48ead
*color6: #96b5b4
*color7: #c0c5ce

*color8: #65737e
*color9: #bf616a
*color10: #a3be8c
*color11: #ebcb8b
*color12: #8fa1b3
*color13: #b48ead
*color14: #96b5b4
*color15: #eff1f5

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
*color16: #d08770
*color17: #ab7967
*color18: #343d46
*color19: #4f5b66
*color20: #a7adba
*color21: #dfe1e8

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! Xterm
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
XTerm.vt100.faceName: Fira Code:size=10

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! URxvt
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
URxvt.font: xft:Fira Code:size=10
URxvt.termName: rxvt-unicode
*imLocale: en_US.UTF8
*saveLines: 5000
*.letterSpace: 1

! Scrollbar
URxvt.scrollBar: false

! Border
URxvt.borderLess: false
URxvt.internalBorder: 10

urxvt*depth: 32
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! Emacs
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
emacs.font: Fira Code-10
emacs.background: #2b303b
emacs*toolBar: 0

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! Rofi
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
rofi.font: Fira Code 10
rofi.modi: combi
rofi.combi-modi: window,drun,run

! "Border width"
rofi.bw: 10

! "Padding" Set from: Default
rofi.padding: INTERNAL_PADDING

! "Number of lines" Set from: Default
rofi.lines: 20

! Set the desired separator style
rofi.separator-style: solid

! Enable the extended coloring options
rofi.color-enabled: true
! Property Name bg border sep
rofi.color-window: #2b303b, #65737e, #2b303b
! Property Name bg fg bgalt hibg hifg
rofi.color-normal: #2b303b, #c0c5ce, #2b303b, #343d46, #eff1f5
rofi.color-active: #2b303b, #8fa1b3, #2b303b, #343d46, #8fa1b3
rofi.color-urgent: #2b303b, #bf616a, #2b303b, #343d46, #bf616a

dunst.c1: #2b303b
dunst.c2: #343d46
